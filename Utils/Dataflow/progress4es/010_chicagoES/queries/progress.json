{
  "size": 0,
  "query": {
    "bool": {
      "must": [
        {"term": {"jobstatus": "finished"}},
        {"term": {"prodsourcelabel": "managed"}},
        {"range": {"statechangetime": {"gte": %(left_ts)s, "lt": %(right_ts)s}}},
        {"range": {"nevents": {"gt": 0}}}
      ]}},
  "aggs": {
    "progress": {
      "composite": {
        "size": 1000,
        "sources": [
          {"date": {
            "date_histogram": {
              "field": "statechangetime",
              "fixed_interval": "1d"
            }
          }},
          {"task": {
            "terms": {"field": "jeditaskid"}
          }}
        ]
      },
      "aggs": {"events": {"sum": {"field": "nevents"}}}
    }
  }
}
